{
  "swagger": "2.0",
  "info": {
    "title": "file/file.proto",
    "version": "version not set"
  },
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/cmdb/downloadTemplate": {
      "get": {
        "summary": "获取导入模板",
        "operationId": "DownloadTemplate",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/fileDownloadTemplateResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "template_type",
            "description": "导入类型.\n\n - UNDEFINED: 0-undefined,指除了关系链模板之外的导入模板，也可不传\n - HOSTCHAIN: 1-导入主机业务拓扑\n - LAKECHAIN: 2-导入lake节点关系链\n - CROSSTABLE: 3-导入交维表",
            "in": "query",
            "required": false,
            "type": "string",
            "enum": [
              "UNDEFINED",
              "HOSTCHAIN",
              "LAKECHAIN",
              "CROSSTABLE"
            ],
            "default": "UNDEFINED"
          },
          {
            "name": "asset_type",
            "description": "非关系链的导入模板的类型.",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "File"
        ]
      }
    },
    "/v1/file/upload": {
      "post": {
        "summary": "上传csv等通用文件",
        "operationId": "Upload",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/fileUploadResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/fileUploadRequest"
            }
          }
        ],
        "tags": [
          "File"
        ]
      }
    },
    "/v1/file/uploadPlaybook": {
      "post": {
        "summary": "上传playbook压缩包并且解析入口yml文件",
        "operationId": "UploadPlaybook",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/fileUploadPlaybookResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/fileUploadPlaybookRequest"
            }
          }
        ],
        "tags": [
          "File"
        ]
      }
    }
  },
  "definitions": {
    "commonResponseStatus": {
      "type": "object",
      "properties": {
        "code": {
          "$ref": "#/definitions/commonStatusCode",
          "title": "状态码"
        },
        "message": {
          "type": "string",
          "title": "信息"
        }
      },
      "title": "请求返回状态"
    },
    "commonStatusCode": {
      "type": "string",
      "enum": [
        "SUCCESS",
        "INVALID_ARGUMENT",
        "ACCESS_DENIED"
      ],
      "default": "SUCCESS",
      "description": "- SUCCESS: 成功\n - INVALID_ARGUMENT: 参数错误\n - ACCESS_DENIED: 访问拒绝",
      "title": "请求返回状态码"
    },
    "fileDownloadTemplateResponse": {
      "type": "object",
      "properties": {
        "status": {
          "$ref": "#/definitions/commonResponseStatus",
          "title": "返回的请求状态"
        },
        "content": {
          "type": "string",
          "format": "byte",
          "title": "返回的excel文件"
        }
      }
    },
    "fileTemplateType": {
      "type": "string",
      "enum": [
        "UNDEFINED",
        "HOSTCHAIN",
        "LAKECHAIN",
        "CROSSTABLE"
      ],
      "default": "UNDEFINED",
      "description": "- UNDEFINED: 0-undefined,指除了关系链模板之外的导入模板，也可不传\n - HOSTCHAIN: 1-导入主机业务拓扑\n - LAKECHAIN: 2-导入lake节点关系链\n - CROSSTABLE: 3-导入交维表",
      "title": "下载模板类型"
    },
    "fileUploadPlaybookRequest": {
      "type": "object",
      "properties": {
        "content": {
          "type": "string",
          "format": "byte",
          "title": "上传的playbook压缩包"
        }
      },
      "title": "上传playbook压缩包并且解析入口yml文件的请求体"
    },
    "fileUploadPlaybookResponse": {
      "type": "object",
      "properties": {
        "url": {
          "type": "string",
          "title": "解析出来的playbook项目存储在对象存储的url"
        },
        "md5": {
          "type": "string",
          "title": "解析出来的playbook项目文件md5"
        },
        "filesize": {
          "type": "string",
          "title": "解析出来的playbook项目文件大小"
        },
        "entrypoint": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "title": "解析出来的playbook项目入口yml文件，有多个"
        },
        "status": {
          "$ref": "#/definitions/commonResponseStatus",
          "title": "返回的请求状态"
        }
      }
    },
    "fileUploadRequest": {
      "type": "object",
      "properties": {
        "content": {
          "type": "string",
          "format": "byte",
          "title": "文件字段"
        },
        "bucket": {
          "type": "string",
          "title": "指定上传bucket_name（可选）"
        },
        "key": {
          "type": "string",
          "title": "指定上传存储路径（可选）"
        }
      },
      "title": "上传csv等通用文件"
    },
    "fileUploadResponse": {
      "type": "object",
      "properties": {
        "url": {
          "type": "string",
          "title": "通用文件对象存储的url"
        },
        "md5": {
          "type": "string",
          "title": "解析出来的通用文件md5"
        },
        "filesize": {
          "type": "string",
          "title": "解析出来的通用文件大小"
        },
        "filename": {
          "type": "string",
          "title": "解析出来的通用文件name"
        },
        "status": {
          "$ref": "#/definitions/commonResponseStatus",
          "title": "返回的请求状态"
        }
      }
    }
  }
}
