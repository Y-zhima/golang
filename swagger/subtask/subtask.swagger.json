        },
        "parameters": [
          {
            "name": "sub_task_id",
            "description": "作业子任务ID",
            "in": "path",
            "required": true,
            "type": "string",
            "format": "int64"
          }
        ],
        "tags": [
          "SubTask"
        ]
      }
    }
  },
