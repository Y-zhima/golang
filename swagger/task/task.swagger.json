{
  "swagger": "2.0",
  "info": {
    "title": "task/task.proto",
    "version": "version not set"
  },
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/task/checkServerState": {
      "post": {
        "summary": "调用裸金属应用检查裸金属状态",
        "operationId": "CheckServerState",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/taskCheckServerStateResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/taskCheckServerStateRequest"
            }
          }
        ],
        "tags": [
          "Task"
        ]
      }
    },
    "/v1/task/create": {
      "post": {
        "summary": "创建作业任务(执行作业模板)",
        "operationId": "Create",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/taskCreateResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/taskCreateRequest"
            }
          }
        ],
        "tags": [
          "Task"
        ]
      }
    },
    "/v1/task/createServer": {
      "post": {
        "summary": "调用裸金属创建操作",
        "operationId": "CreateServer",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/taskCreateServerResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/taskCreateServerRequest"
            }
          }
        ],
        "tags": [
          "Task"
        ]
      }
    },
    "/v1/task/filter": {
      "get": {
        "summary": "筛选作业任务",
        "operationId": "Filter",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/taskFilterResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "paging.total_page",
            "description": "总页数.",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "paging.page",
            "description": "当前页数.",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "paging.per_page",
            "description": "每页显示的记录条数.",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "paging.total_record",
            "description": "总记录数.",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "task_name",
            "description": "用于筛选任务名字的关键字.",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "task_type",
            "description": "用于筛选定时任务的任务类型 0-undefined 1-作业模板；2-容器部署；3-裸金属管理.\n\n - UNDEFINED: 0-undefined\n - PLAYBOOK: 1-作业模板\n - CONTAINER: 2-容器部署\n - BARE_METAL: 3-裸金属管理",
            "in": "query",
            "required": false,
            "type": "string",
            "enum": [
              "UNDEFINED",
              "PLAYBOOK",
              "CONTAINER",
              "BARE_METAL"
            ],
            "default": "UNDEFINED"
          }
        ],
        "tags": [
          "Task"
        ]
      }
    },
    "/v1/task/get/{task_id}": {
      "get": {
        "summary": "获取作业任务",
        "operationId": "Get",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/taskGetResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "task_id",
            "description": "作业任务ID",
            "in": "path",
            "required": true,
            "type": "string",
            "format": "int64"
          }
        ],
        "tags": [
          "Task"
        ]
      }
    },
    "/v1/task/installServer": {
      "post": {
        "summary": "调用裸金属安装操作",
        "operationId": "InstallServer",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/taskInstallServerResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/taskInstallServerRequest"
            }
          }
        ],
        "tags": [
          "Task"
        ]
      }
    },
    "/v1/task/serverPowerControl": {
      "post": {
        "summary": "调用裸金属应用执行开关机操作",
        "operationId": "ServerPowerControl",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/taskServerPowerControlResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/taskServerPowerControlRequest"
            }
          }
        ],
        "tags": [
          "Task"
        ]
      }
    }
  },
  "definitions": {
    "cmdbChooseHostRequest": {
      "type": "object",
      "properties": {
        "bk_set_id": {
          "type": "integer",
          "format": "int32",
          "title": "集群ID"
        },
        "bk_module_id": {
          "type": "array",
          "items": {
            "type": "integer",
            "format": "int32"
          },
          "title": "模块ID"
        }
      },
      "title": "选择主机请求"
    },
    "cmdbChooseServerRequest": {
      "type": "object",
      "properties": {
        "room_id": {
          "type": "integer",
          "format": "int32",
          "title": "机房ID"
        },
        "server": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/cmdbServer"
          },
          "title": "机房下对应物理机UUID及ipmi的ip"
        }
      },
      "title": "选择物理机请求"
    },
    "cmdbServer": {
      "type": "object",
      "properties": {
        "uuid": {
          "type": "string",
          "title": "裸金属uuid"
        },
        "ipmi_ip": {
          "type": "string",
          "title": "物理机ipmi的ip"
        }
      },
      "title": "物理机传入信息"
    },
    "cmdbServerPowerState": {
      "type": "string",
      "enum": [
        "UNKNOWN",
        "OFF",
        "ON"
      ],
      "default": "UNKNOWN",
      "description": "- UNKNOWN: 未知状态\n - OFF: 下电\n - ON: 上电",
      "title": "电源状态"
    },
    "commonPaging": {
      "type": "object",
      "properties": {
        "total_page": {
          "type": "integer",
          "format": "int32",
          "title": "总页数"
        },
        "page": {
          "type": "integer",
          "format": "int32",
          "title": "当前页数"
        },
        "per_page": {
          "type": "integer",
          "format": "int32",
          "title": "每页显示的记录条数"
        },
        "total_record": {
          "type": "integer",
          "format": "int32",
          "title": "总记录数"
        }
      },
      "title": "分页"
    },
    "commonResponseStatus": {
      "type": "object",
      "properties": {
        "code": {
          "$ref": "#/definitions/commonStatusCode",
          "title": "状态码"
        },
        "message": {
          "type": "string",
          "title": "信息"
        }
      },
      "title": "请求返回状态"
    },
    "commonStatusCode": {
      "type": "string",
      "enum": [
        "SUCCESS",
        "INVALID_ARGUMENT",
        "ACCESS_DENIED"
      ],
      "default": "SUCCESS",
      "description": "- SUCCESS: 成功\n - INVALID_ARGUMENT: 参数错误\n - ACCESS_DENIED: 访问拒绝",
      "title": "请求返回状态码"
    },
    "scheduleTaskType": {
      "type": "string",
      "enum": [
        "UNDEFINED",
        "PLAYBOOK",
        "CONTAINER",
        "BARE_METAL"
      ],
      "default": "UNDEFINED",
      "description": "- UNDEFINED: 0-undefined\n - PLAYBOOK: 1-作业模板\n - CONTAINER: 2-容器部署\n - BARE_METAL: 3-裸金属管理",
      "title": "任务类型 0-undefined 1-作业模板；2-容器部署 3-裸金属管理"
    },
    "taskCheckServerStateRequest": {
      "type": "object",
      "properties": {
        "cmdb_search_request": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/cmdbChooseServerRequest"
          },
          "title": "机房及对应物理机"
        }
      },
      "title": "调用裸金属应用检查裸金属状态请求体"
    },
    "taskCheckServerStateResponse": {
      "type": "object",
      "properties": {
        "status": {
          "$ref": "#/definitions/commonResponseStatus",
          "title": "状态码"
        }
      },
      "title": "调用裸金属应用检查裸金属状态返回体"
    },
    "taskCreateRequest": {
      "type": "object",
      "properties": {
        "template_id": {
          "type": "integer",
          "format": "int32",
          "title": "模板ID"
        },
        "cmdb_search_request": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/cmdbChooseHostRequest"
          },
          "title": "cmdb的搜索条件"
        },
        "task_type": {
          "$ref": "#/definitions/scheduleTaskType",
          "title": "任务类型"
        },
        "extra_var": {
          "type": "string",
          "title": "额外变量JSON String 例如： {\"key\":\"testKey\",\"value\":\"testVal\",\"description\":\"测试描述\"}"
        },
        "schedule_id": {
          "type": "integer",
          "format": "int32",
          "title": "定时任务ID"
        },
        "schedule_type": {
          "$ref": "#/definitions/taskScheduleType",
          "title": "定时任务类型"
        },
        "name": {
          "type": "string",
          "title": "作业名称"
        }
      },
      "title": "创建任务实例请求",
      "required": [
        "schedules",
        "cmdb_search_request"
      ]
    },
    "taskCreateResponse": {
      "type": "object",
      "properties": {
        "task_id": {
          "type": "string",
          "format": "int64",
          "title": "任务实例ID"
        },
        "status": {
          "$ref": "#/definitions/commonResponseStatus",
          "title": "返回的请求状态"
        }
      },
      "title": "创建任务实例请求返回"
    },
    "taskCreateServerRequest": {
      "type": "object",
      "properties": {
        "cmdb_search_request": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/cmdbChooseServerRequest"
          },
          "title": "机房及对应物理机"
        }
      },
      "title": "调用裸金属创建操作请求体"
    },
    "taskCreateServerResponse": {
      "type": "object",
      "properties": {
        "status": {
          "$ref": "#/definitions/commonResponseStatus",
          "title": "状态码"
        }
      },
      "title": "调用裸金属创建操作返回体"
    },
    "taskFilterResponse": {
      "type": "object",
      "properties": {
        "tasks": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/taskTaskObject"
          },
          "title": "筛选到的多个作业任务实例"
        },
        "paging": {
          "$ref": "#/definitions/commonPaging",
          "title": "分页信息"
        },
        "status": {
          "$ref": "#/definitions/commonResponseStatus",
          "title": "返回的请求状态"
        }
      },
      "title": "筛选作业任务请求返回"
    },
    "taskGetLogResponse": {
      "type": "object",
      "properties": {
        "status": {
          "$ref": "#/definitions/commonResponseStatus",
          "title": "返回的请求状态"
        }
      },
      "title": "获取作业任务详细执行过程请求返回"
    },
    "taskGetResponse": {
      "type": "object",
      "properties": {
        "task": {
          "$ref": "#/definitions/taskTaskObject",
          "title": "获取的作业任务实例"
        },
        "playbook_version": {
          "type": "string",
          "title": "task对于的作业版本"
        },
        "template_name": {
          "type": "string",
          "title": "作业对应的模板名字"
        },
        "host_module": {
          "type": "string",
          "title": "task对于的主机模块"
        },
        "status": {
          "$ref": "#/definitions/commonResponseStatus",
          "title": "返回的请求状态"
        }
      },
      "title": "获取作业任务请求返回"
    },
    "taskInstallServerRequest": {
      "type": "object",
      "properties": {
        "cmdb_search_request": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/cmdbChooseServerRequest"
          },
          "title": "机房及对应物理机"
        },
        "image_file_url": {
          "type": "string",
          "title": "安装镜像文件的URL"
        },
        "image_file_md5": {
          "type": "string",
          "title": "安装镜像文件的MD5"
        },
        "cmdb__host_search_request": {
          "$ref": "#/definitions/cmdbChooseHostRequest",
          "title": "主机查询条件"
        }
      },
      "title": "调用裸金属安装操作请求体"
    },
    "taskInstallServerResponse": {
      "type": "object",
      "properties": {
        "status": {
          "$ref": "#/definitions/commonResponseStatus",
          "title": "状态码"
        }
      },
      "title": "调用裸金属安装操作返回体"
    },
    "taskScheduleType": {
      "type": "string",
      "enum": [
        "UNDEFINED",
        "SINGLE",
        "CRONTAB"
      ],
      "default": "UNDEFINED",
      "description": "- UNDEFINED: 0-undefined\n - SINGLE: 1-单次任务\n - CRONTAB: 2-定时任务",
      "title": "定时任务类型 0-undefined 1-单次任务 2-定时任务"
    },
    "taskServerPowerControlRequest": {
      "type": "object",
      "properties": {
        "state": {
          "$ref": "#/definitions/cmdbServerPowerState",
          "title": "电源开关操作"
        },
        "cmdb_search_request": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/cmdbChooseServerRequest"
          },
          "title": "机房及对应物理机"
        }
      },
      "title": "调用裸金属应用执行开关机操作请求体"
    },
    "taskServerPowerControlResponse": {
      "type": "object",
      "properties": {
        "status": {
          "$ref": "#/definitions/commonResponseStatus",
          "title": "状态码"
        }
      },
      "title": "调用裸金属应用执行开关机操作返回体"
    },
    "taskTaskObject": {
      "type": "object",
      "properties": {
        "task_id": {
          "type": "string",
          "format": "int64",
          "title": "作业任务ID"
        },
        "template_id": {
          "type": "integer",
          "format": "int32",
          "title": "模板ID"
        },
        "schedule_id": {
          "type": "integer",
          "format": "int32",
          "title": "定时任务ID"
        },
        "schedule_type": {
          "$ref": "#/definitions/taskScheduleType",
          "title": "定时任务类型"
        },
        "task_type": {
          "$ref": "#/definitions/scheduleTaskType",
          "title": "任务类型"
        },
        "cmdb_search_request": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/cmdbChooseHostRequest"
          },
          "title": "cmdb的搜索条件"
        },
        "extra_var": {
          "type": "string",
          "title": "额外变量JSON String 例如： {\"key\":\"testKey\",\"value\":\"testVal\",\"description\":\"测试描述\"}"
        },
        "executor": {
          "type": "integer",
          "format": "int32",
          "title": "执行人ID"
        },
        "execute_count": {
          "type": "integer",
          "format": "int32",
          "title": "总共执行多少主机"
        },
        "fail_count": {
          "type": "integer",
          "format": "int32",
          "title": "执行失败多少台主机"
        },
        "success_count": {
          "type": "integer",
          "format": "int32",
          "title": "执行成功多少台主机"
        },
        "start_time": {
          "type": "string",
          "title": "执行开始时间"
        },
        "end_time": {
          "type": "string",
          "title": "执行结束时间"
        },
        "name": {
          "type": "string",
          "title": "作业名称"
        }
      },
      "title": "作业任务实例"
    }
  }
}
