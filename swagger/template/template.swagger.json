{
  "swagger": "2.0",
  "info": {
    "title": "template/template.proto",
    "version": "version not set"
  },
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/template/create": {
      "post": {
        "summary": "创建作业模板",
        "operationId": "Create",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/templateCreateResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/templateCreateRequest"
            }
          }
        ],
        "tags": [
          "Template"
        ]
      }
    },
    "/v1/template/filter": {
      "get": {
        "summary": "筛选作业模板",
        "operationId": "Filter",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/templateFilterResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "paging.total_page",
            "description": "总页数.",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "paging.page",
            "description": "当前页数.",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "paging.per_page",
            "description": "每页显示的记录条数.",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "paging.total_record",
            "description": "总记录数.",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "name",
            "description": "模板名的一部分，模糊查询时的关键字.",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "template_id",
            "description": "模板ID.",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "Template"
        ]
      }
    },
    "/v1/template/get/{template_id}": {
      "get": {
        "summary": "获取作业模板",
        "operationId": "Get",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/templateGetResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "template_id",
            "description": "模板ID",
            "in": "path",
            "required": true,
            "type": "integer",
            "format": "int32"
          }
        ],
        "tags": [
          "Template"
        ]
      }
    },
    "/v1/template/update/{template_id}": {
      "post": {
        "summary": "更新作业模板",
        "operationId": "Update",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/templateUpdateResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "template_id",
            "description": "模板ID",
            "in": "path",
            "required": true,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/templateUpdateRequest"
            }
          }
        ],
        "tags": [
          "Template"
        ]
      }
    }
  },
  "definitions": {
    "commonPaging": {
      "type": "object",
      "properties": {
        "total_page": {
          "type": "integer",
          "format": "int32",
          "title": "总页数"
        },
        "page": {
          "type": "integer",
          "format": "int32",
          "title": "当前页数"
        },
        "per_page": {
          "type": "integer",
          "format": "int32",
          "title": "每页显示的记录条数"
        },
        "total_record": {
          "type": "integer",
          "format": "int32",
          "title": "总记录数"
        }
      },
      "title": "分页"
    },
    "commonResponseStatus": {
      "type": "object",
      "properties": {
        "code": {
          "$ref": "#/definitions/commonStatusCode",
          "title": "状态码"
        },
        "message": {
          "type": "string",
          "title": "信息"
        }
      },
      "title": "请求返回状态"
    },
    "commonStatusCode": {
      "type": "string",
      "enum": [
        "SUCCESS",
        "INVALID_ARGUMENT",
        "ACCESS_DENIED"
      ],
      "default": "SUCCESS",
      "description": "- SUCCESS: 成功\n - INVALID_ARGUMENT: 参数错误\n - ACCESS_DENIED: 访问拒绝",
      "title": "请求返回状态码"
    },
    "commonUser": {
      "type": "object",
      "properties": {
        "user_id": {
          "type": "integer",
          "format": "int32"
        },
        "user_name": {
          "type": "string"
        }
      }
    },
    "playbookPlaybookEntrypointObject": {
      "type": "object",
      "properties": {
        "playbook_entrypoint_id": {
          "type": "integer",
          "format": "int32",
          "title": "入口文件ID"
        },
        "name": {
          "type": "string",
          "title": "入口文件名称"
        }
      },
      "title": "playbook入口文件实例"
    },
    "playbookProjectObject": {
      "type": "object",
      "properties": {
        "playbook_id": {
          "type": "integer",
          "format": "int32",
          "title": "playbook项目ID"
        },
        "name": {
          "type": "string",
          "title": "playbook项目名称"
        },
        "description": {
          "type": "string",
          "title": "playbook项目描述"
        },
        "url": {
          "type": "string",
          "title": "playbook存储在对象存储的url"
        },
        "url_type": {
          "$ref": "#/definitions/playbookUrlType",
          "title": "枚举类型 1-文件下载 2-git下载"
        },
        "version": {
          "type": "string",
          "title": "playbook文件的版本号"
        },
        "entrypoint": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/playbookPlaybookEntrypointObject"
          },
          "title": "playbook入口yml文件，有多个"
        },
        "md5": {
          "type": "string",
          "title": "文件md5"
        },
        "size": {
          "type": "string",
          "format": "int64",
          "title": "文件大小"
        },
        "created": {
          "type": "string",
          "title": "创建时间"
        },
        "updated": {
          "type": "string",
          "title": "更新时间"
        },
        "playbook_file_id": {
          "type": "integer",
          "format": "int32",
          "title": "playbook项目文件ID"
        },
        "creator": {
          "$ref": "#/definitions/commonUser",
          "title": "创建人"
        }
      },
      "title": "项目实例"
    },
    "playbookUrlType": {
      "type": "string",
      "enum": [
        "UNDEFINED",
        "FILE",
        "GIT"
      ],
      "default": "UNDEFINED",
      "description": "- UNDEFINED: 未定义\n - FILE: 文件下载\n - GIT: git下载",
      "title": "枚举URL类型"
    },
    "templateCreateRequest": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "title": "模板名"
        },
        "description": {
          "type": "string",
          "title": "模板描述"
        },
        "playbook_id": {
          "type": "integer",
          "format": "int32",
          "title": "playbook 项目id"
        },
        "playbook_file_id": {
          "type": "integer",
          "format": "int32",
          "title": "选择的playbook项目对应的playbook_file"
        },
        "playbook_entrypoint_id": {
          "type": "integer",
          "format": "int32",
          "title": "选择的playbook入口yml文件"
        },
        "extra_var": {
          "type": "string",
          "title": "额外变量JSON String 例如： {\"key\":\"testKey\",\"value\":\"testVal\",\"description\":\"测试描述\"}"
        }
      },
      "title": "创建模板请求",
      "required": [
        "name",
        "playbook_id",
        "playbook_file_id",
        "playbook_entrypoint_id"
      ]
    },
    "templateCreateResponse": {
      "type": "object",
      "properties": {
        "template_id": {
          "type": "integer",
          "format": "int32",
          "title": "模板ID"
        },
        "created": {
          "type": "string",
          "title": "模板创建的时间"
        },
        "status": {
          "$ref": "#/definitions/commonResponseStatus",
          "title": "请求返回状态"
        }
      },
      "title": "创建模板请求返回"
    },
    "templateDeleteResponse": {
      "type": "object",
      "properties": {
        "status": {
          "$ref": "#/definitions/commonResponseStatus",
          "title": "请求返回的状态"
        }
      },
      "title": "删除请求返回"
    },
    "templateFilterResponse": {
      "type": "object",
      "properties": {
        "templates": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/templateTemplateObject"
          },
          "title": "请求返回的模板"
        },
        "paging": {
          "$ref": "#/definitions/commonPaging",
          "title": "分页信息"
        },
        "status": {
          "$ref": "#/definitions/commonResponseStatus",
          "title": "请求返回状态"
        }
      },
      "title": "筛选模板请求返回"
    },
    "templateGetResponse": {
      "type": "object",
      "properties": {
        "template": {
          "$ref": "#/definitions/templateTemplateObject",
          "title": "请求返回的模板"
        },
        "status": {
          "$ref": "#/definitions/commonResponseStatus",
          "title": "请求返回状态"
        }
      },
      "title": "获取模板请求返回"
    },
    "templateTemplateObject": {
      "type": "object",
      "properties": {
        "template_id": {
          "type": "integer",
          "format": "int32",
          "title": "模板ID"
        },
        "name": {
          "type": "string",
          "title": "模板名"
        },
        "description": {
          "type": "string",
          "title": "模板描述"
        },
        "playbook": {
          "$ref": "#/definitions/playbookProjectObject",
          "title": "playbook 项目"
        },
        "playbook_file_id": {
          "type": "integer",
          "format": "int32",
          "title": "选择的playbook项目对应的playbook_file"
        },
        "playbook_entrypoint_id": {
          "type": "integer",
          "format": "int32",
          "title": "选择的playbook入口yml文件"
        },
        "extra_var": {
          "type": "string",
          "title": "额外变量"
        },
        "creator": {
          "$ref": "#/definitions/commonUser",
          "title": "创建人"
        },
        "created": {
          "type": "string",
          "title": "创建时间"
        },
        "updated": {
          "type": "string",
          "title": "更新时间"
        }
      },
      "title": "模板实例"
    },
    "templateUpdateRequest": {
      "type": "object",
      "properties": {
        "template_id": {
          "type": "integer",
          "format": "int32",
          "title": "模板ID"
        },
        "name": {
          "type": "string",
          "title": "模板名"
        },
        "description": {
          "type": "string",
          "title": "模板描述"
        },
        "playbook_id": {
          "type": "integer",
          "format": "int32",
          "title": "playbook项目的id"
        },
        "playbook_file_id": {
          "type": "integer",
          "format": "int32",
          "title": "所选playbook项目对应的playbookFile的id"
        },
        "playbook_entrypoint_id": {
          "type": "integer",
          "format": "int32",
          "title": "所选playbook项目对应的入口yml文件的id"
        },
        "extra_var": {
          "type": "string",
          "title": "额外变量JSON String 例如： {\"key\":\"testKey\",\"value\":\"testVal\",\"description\":\"测试描述\"}"
        }
      },
      "title": "更新模板请求",
      "required": [
        "template_id",
        "name",
        "playbook_id",
        "playbook_file_id",
        "playbook_entrypoint_id"
      ]
    },
    "templateUpdateResponse": {
      "type": "object",
      "properties": {
        "updated": {
          "type": "string",
          "title": "模板更新的时间"
        },
        "status": {
          "$ref": "#/definitions/commonResponseStatus",
          "title": "请求返回的状态"
        }
      },
      "title": "更新请求返回"
    }
  }
}
